{{#if anyItems}}
<table class="table table-striped">
  {{#each model}}
  <tr>
    <td>
      {{topic.name}}
    </td>
    <td>
      {{left.value}}
    </td>
    <td>
      vs
    </td>
    <td>
      {{right.value}}
    </td>
  </tr>
  {{/each}}
</table>
{{else}}
  <span style="position:relative; top:10px;">No arguments found.</span>
{{/if}}

